{
  "version": 2,
  "functions": {
    "api/*.js": {
      "includeFiles": "public/**,src/**,data/**,config/**"
    }
  },
  "rewrites": [
    {
      "source": "/health",
      "destination": "/api/health"
    },
    {
      "source": "/",
      "destination": "/api/landing"
    },
    {
      "source": "/(.*)",
      "destination": "/api/index.js"
    }
  ]
}
