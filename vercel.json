{
  "version": 2,
  "functions": {
    "api/*.js": {
      "includeFiles": "public/**,src/**,data/**,config/**"
    }
  },
  "rewrites": [
    { "source": "/", "destination": "/api/landing" },
    { "source": "/health", "destination": "/api/health" },
    { "source": "/(.*)", "destination": "/api/index.js" }
  ]
}
