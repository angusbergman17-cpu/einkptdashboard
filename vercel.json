{
  "version": 2,
  "functions": {
    "api/index.js": {
      "includeFiles": "public/**,src/**,data/**,config/**"
    }
  },
  "redirects": [
    {
      "source": "/",
      "destination": "/admin",
      "permanent": false
    }
  ],
  "rewrites": [
    {
      "source": "/health",
      "destination": "/api/health"
    },
    {
      "source": "/(.*)",
      "destination": "/api/index.js"
    }
  ]
}
